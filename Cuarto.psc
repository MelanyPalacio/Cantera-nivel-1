Proceso CuartaAct
	Definir nombreCiudad Como Caracter;
	Definir nombrePais Como Caracter;
	
	Escribir "Escriba una ciudad";
	Leer nombreCiudad;
	
	Escribir "Escriba un pais";
	Leer nombrePais;
	
	Escribir "La ciudad ", nombreCiudad, " Es la capital del pais ", nombrePais; 
	
FinProceso
